{
	"name": "nodejs-template",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"commit": "cz",
		"pre-commit": "lint-staged",
		"prettier:check": "prettier --check .",
		"prettier:fix": "prettier --write .",
		"eslint:check": "eslint .",
		"lint": "npm run eslint:check && npm run prettier:check",
		"lint:fix": "npm run eslint:check && npm run prettier:fix",
		"prepare": "husky",
		"start": "npm run compile && node --env-file=.env built/src/index.js",
		"dev": "tsx watch --no-warnings=ExperimentalWarning --env-file=.env src/index.ts",
		"compile": "rm -rf ./built && tsc"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx,json,css,html}": "prettier --write",
		"*.{js,jsx,ts,tsx}": "eslint"
	},
	"devDependencies": {
		"@commitlint/cli": "^19.3.0",
		"@commitlint/config-conventional": "^19.2.2",
		"@eslint/js": "^9.3.0",
		"@stylistic/eslint-plugin": "^2.1.0",
		"@types/tmi.js": "^1.8.6",
		"commitizen": "^4.3.0",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^9.3.0",
		"globals": "^15.2.0",
		"husky": "^9.0.11",
		"lint-staged": "^15.2.2",
		"nodemon": "^3.1.0",
		"prettier": "^3.2.5",
		"tsx": "^4.10.5",
		"typescript": "^5.4.5",
		"typescript-eslint": "^8.0.0-alpha.13"
	},
	"type": "module",
	"repository": {
		"type": "git",
		"url": "https://github.com/Bekert/nodejs-template"
	},
	"author": "Bekert",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/Bekert/nodejs-template/issues"
	},
	"homepage": "https://github.com/Bekert/nodejs-template",
	"dependencies": {
		"mongoose": "^8.4.0",
		"openai": "^4.47.1",
		"pino": "^9.1.0",
		"tmi.js": "^1.8.5"
	}
}
